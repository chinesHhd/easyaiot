    <select id="selectProductServicesList"
            parameterType="com.basiclab.iot.device.domain.device.vo.ProductServices"
            resultMap="BaseResultMap">
        <include refid="selectProductServicesVo"/>
        <where>
            <if test="serviceCode != null  and serviceCode != ''">
                and service_code like concat('%', #{serviceCode}::varchar, '%')
            </if>
            <if test="serviceName != null  and serviceName != ''">
                and service_name like concat('%', #{serviceName}::varchar, '%')
            </if>
            <choose>
                <when test="templateIdentification != null and productIdentification != null">
                    and (template_identification = #{templateIdentification} or product_identification = #{productIdentification})
                </when>
                <when test="templateIdentification != null">
                    and template_identification = #{templateIdentification}
                </when>
                <when test="productIdentification != null">
                    and product_identification = #{productIdentification}
                </when>
                <otherwise></otherwise>
            </choose>
            <if test="status != null  and status != ''">
                and status = #{status}
            </if>
            <if test="description != null  and description != ''">
                and description = #{description}
            </if>
        </where>
        <include refid="OrderByCreateTimeDesc"/>
    </select>

