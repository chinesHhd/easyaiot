    <select id="selectProductPropertiesList"
            parameterType="com.basiclab.iot.device.domain.device.vo.ProductProperties"
            resultMap="BaseResultMap">
        <include refid="selectProductPropertiesVo"/>
        <where>
            <choose>
                <when test="propertyCode != null and propertyName != null">
                    and (property_code = #{propertyCode} or property_name = #{propertyName})
                </when>
                <when test="propertyCode != null and propertyCode != ''">
                    and property_code = #{propertyCode}
                </when>
                <when test="propertyName != null and propertyName != ''">
                    and property_name = #{propertyName}
                </when>
            </choose>
            <choose>
                <when test="templateIdentification != null and productIdentification != null">
                    and (template_identification = #{templateIdentification} or product_identification = #{productIdentification})
                </when>
                <when test="templateIdentification != null">
                    and template_identification = #{templateIdentification}
                </when>
                <when test="productIdentification != null">
                    and product_identification = #{productIdentification}
                </when>
            </choose>
            <if test="datatype != null  and datatype != ''">
                and datatype = #{datatype}
            </if>
            <if test="description != null  and description != ''">
                and description = #{description}
            </if>
            <if test="enumlist != null  and enumlist != ''">
                and enumlist = #{enumlist}
            </if>
            <if test="max != null  and max != ''">
                and max = #{max}
            </if>
            <if test="maxlength != null">
                and maxlength = #{maxlength}
            </if>
            <if test="method != null  and method != ''">
                and method = #{method}
            </if>
            <if test="min != null  and min != ''">
                and min = #{min}
            </if>
            <if test="required != null">
                and required = #{required}
            </if>
            <if test="step != null">
                and step = #{step}
            </if>
            <if test="unit != null  and unit != ''">
                and unit = #{unit}
            </if>
        </where>
        order by update_time desc
    </select>

    <select id="selectProductPropertiesById" parameterType="Long" resultMap="BaseResultMap">
